{
  "name": "puppy-lib-components",
  "version": "1.3.3",
  "description": "A modern TypeScript React component library with generic UI components and football pickem domain components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/styles/globals.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && npm run copy-css",
    "copy-css": "cp -r src/styles dist/ && find src -name '*.css' -not -path 'src/styles/*' -exec bash -c 'mkdir -p dist/$(dirname \"{}\" | sed \"s|^src/||\" ) && cp \"{}\" dist/$(dirname \"{}\" | sed \"s|^src/||\")/$(basename \"{}\")' \\;",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prepack": "npm run build"
  },
  "keywords": [
    "react",
    "typescript",
    "components",
    "ui-library",
    "design-system",
    "football",
    "pickem",
    "fantasy-sports"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/lib-components.git"
  },
  "homepage": "https://github.com/your-username/lib-components#readme",
  "bugs": {
    "url": "https://github.com/your-username/lib-components/issues"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/x-date-pickers-pro": "^8.11.2",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-webpack5": "^8.6.14",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-loader": "^9.1.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.0",
    "storybook": "^8.6.14",
    "typescript": "^5.1.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
